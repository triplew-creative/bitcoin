# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = off

[*.ts]
quote_type = single
end_of_line = next_line_shifted

[*.md]
trim_trailing_whitespace = false